{
	"name"        : "busyphp/alipay-pay",
	"type"        : "library",
	"description" : "用于BusyPHP的支付宝支付，支持wap端、H5端、APP端、PC端支付/退款",
	"homepage"    : "http://www.harter.cn/?form=BusyPHP&plugin=busyphp/alipay-pay",
	"keywords"    : [
		"alipay",
		"支付宝"
	],
	"authors"     : [
		{
			"name"  : "busy^life",
			"email" : "busy.life@qq.com"
		}
	],
	"license"     : "CC-BY-NC-4.0",
	"require"     : {
		"ext-json"            : "*",
		"busyphp/trade"       : "^2.0",
		"busyphp/alipay-base" : "^2.0"
	},
	"require-dev" : {
		"symfony/var-dumper" : "^4.2"
	},
	"autoload"    : {
		"psr-4" : {
			"BusyPHP\\alipay\\pay\\" : "src"
		}
	},
	"config"      : {
		"preferred-install" : "dist"
	},
	"extra"       : {
		"branch-alias" : {
			"dev-master" : "2.0.x-dev"
		},
		"busyphp"      : {
			"manager" : {
				"name" : "支付宝支付模块"
			}
		}
	}
}
